public class primeNumbers {

    public static void main(String[] args) {


        for (int i=2; i<=9; i++){
            if (i%4!=0 && i%6!=0 && i%8!=0){
                System.out.println(i);
            }
        }
        for (int k=2; k<=100; k++){
            if (k%2!=0 && k%3!=0 && k%4!=0 && k%5!=0 && k%6!=0 && k%7!=0 && k%8!=0 && k%9!=0 && k%10!=0){
                System.out.println(k);
            }

        }

    }


}
